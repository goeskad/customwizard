<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.nuxeo.xforms.ui.editors.XFormsEditor"
            default="false"
            extensions="xforms, xform, xf, xhtml"
            icon="icons/obj16/xforms_editor.gif"
            id="org.nuxeo.xforms.ui.editor.XFormsEditor"
            name="XForms Editor"/>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="true"
            class="org.nuxeo.xforms.ui.editors.XFormsView"
            icon="icons/obj16/xforms_editor.gif"
            id="org.nuxeo.xforms.ui.editor.XFormsView"
            name="XForms Browser"/>
   </extension>

</plugin>
